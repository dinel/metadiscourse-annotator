span.sensedummy {
    font-size: 12px;
}

{% autoescape 'html' %}
    {% for sense in senses %}
        span.sense{{sense.id}} {
          background-color: {{ sense.bgcolor }};
          color: {{ sense.fgcolor }};
          border: dotted thin black;
          padding: 2px;
        }
    {% endfor %}
{% endautoescape %}
